<form class="update-form" method="post" id="update-form">
  <label for="firstNameInput">First Name <span class="mandatory">*</span>:</label><br>
  <input type="text" id="firstNameInput" name="firstNameInput" placeholder="Please enter your First Name" value="{{firstName}}" required><br>

  <label for="middleNameInput">Middle Name :</label><br>
  <input type="text" id="middleNameInput" name="middleNameInput" placeholder="Please enter your Middle Name" value="{{middleName}}"><br>

  <label for="lastNameInput">Last Name <span class="mandatory">*</span>:</label><br>
  <input type="text" id="lastNameInput" name="lastNameInput" placeholder="Please enter your Last Name" value="{{lastName}}" required><br>

  <label for="emailAddressInput">Email Address <span class="mandatory">*</span>:</label><br>
  <input type="email" id="emailAddressInput" name="emailAddressInput" placeholder="Please enter your email address" value="{{emailAddress}}" required><br>

  <label for="countryCodeInput">Country Code <span class="mandatory">*</span>:</label><br>
  <input list="countryCodes" id="countryCodeInput" name="countryCodeInput" autocomplete="on" placeholder="{{defaultCountry.flag}} - {{defaultCountry.code}} - ({{defaultCountry.dial_code}}) {{defaultCountry.name}}" value="{{countryCode}}" required>
  <datalist id="countryCodes">
    {{#each countryCodes}}
    <option value="{{dial_code}}-{{name}}">{{flag}} - {{code}} - ({{dial_code}}) {{name}} </option>
    {{/each}}
  </datalist><br>

  <label for="phoneNumberInput">Phone Number <span class="mandatory">*</span>:</label><br>
  <input type="tel" id="phoneNumberInput" name="phoneNumberInput" pattern="[0-9]{10}" placeholder="Please enter your Phone Number" value="{{phoneNumber}}" required><br>


  <label for="cityInput">City <span class="mandatory">*</span>:</label><br>
  <input type="text" id="cityInput" name="cityInput" placeholder="Please enter your City" value="{{city}}" required><br>

  <label for="stateInput">State <span class="mandatory">*</span>:</label><br>
  <input type="text" id="stateInput" name="stateInput" placeholder="Please enter your State" value="{{state}}" required><br>

  <label for="countryInput">Country <span class="mandatory">*</span>:</label><br>
  <input list="countryList" id="countryInput" name="countryInput" autocomplete="on" placeholder="{{defaultCountry.name}}" value="{{country}}" required>
  <datalist id="countryList">
    {{#each countryList}}
    <option value="{{this}}">{{this}}</option>
    {{/each}}
  </datalist><br>

  <label for="dobInput">Date of Birth <span class="mandatory">*</span>:</label><br>
  <input type="date" id="dobInput" name="dobInput" max="{{maxDate}}" value="{{dob}}" >
  <br>
</form>